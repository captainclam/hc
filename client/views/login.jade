form#login-form.login
  .register__window
    .register__group
      input(id="username", autofocus, autocomplete="off",placeholder="Enter your email address").register__input
      label.register__label Username
    .register__group
      input(id="password", type="password", autocomplete="off").register__input
      label.register__label Password
    .register__group
      button(type="submit").button.button--block.button--blue Login
